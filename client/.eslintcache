n            <div>\r\n                <div className=\"titlecard\">\r\n                    <h1>{recipe.title}</h1>\r\n                    {Auth.loggedIn() ? (\r\n                        <>\r\n                            <button onClick={handleAddto} enabled>Add to Cookbook</button>\r\n                        </>\r\n                    ): (\r\n                        <>\r\n                            <button disabled>Add to Cookbook</button>\r\n                        </>\r\n                    )}\r\n                </div>\r\n                <div className=\"generalInfo\">\r\n                    <p>Number of Servings: {recipe.servings}</p>\r\n                    <p>Total Cook Time: {recipe.totalTime}</p>\r\n                </div>\r\n                <h1>Ingredients</h1>\r\n                <div className=\"ingredientsList\">\r\n                    {recipe.ingredients.map((ingredient) => (\r\n                        <p>{ingredient}</p>\r\n                    ))}\r\n                </div>\r\n                <h1>Directions</h1>\r\n                <div className=\"directionsList\">\r\n                    {recipe.directions.map((direction) => (\r\n                        <p>{direction}</p>\r\n                    ))}\r\n                </div>\r\n            </div>\r\n            \r\n        </div>\r\n    );\r\n};\r\n\r\nexport default SingleRecipe;","C:\\Users\\Justin\\Desktop\\Coding\\mycookbook prototype\\mycookbookver1\\client\\src\\pages\\CreateRecipe.js",[],"C:\\Users\\Justin\\Desktop\\Coding\\mycookbook prototype\\mycookbookver2\\client\\src\\index.js",[],["273","274"],"C:\\Users\\Justin\\Desktop\\Coding\\mycookbook prototype\\mycookbookver2\\client\\src\\reportWebVitals.js",[],"C:\\Users\\Justin\\Desktop\\Coding\\mycookbook prototype\\mycookbookver2\\client\\src\\App.js",[],"C:\\Users\\Justin\\Desktop\\Coding\\mycookbook prototype\\mycookbookver2\\client\\src\\pages\\SingleRecipe.js",["275","276","277"],"C:\\Users\\Justin\\Desktop\\Coding\\mycookbook prototype\\mycook